{
  "name": "hd-penny-guide-nextjs",
  "version": "1.0.0",
  "description": "Home Depot Penny Items Guide - Professional Next.js educational platform",
  "private": true,
  "scripts": {
    "dev": "next dev --webpack -p 3001",
    "dev:turbo": "next dev --turbo -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "eslint app components lib --ext .ts,.tsx --max-warnings=0",
    "lint:colors": "tsx scripts/lint-colors.ts",
    "lint:colors:update-baseline": "tsx scripts/lint-colors.ts --update-baseline",
    "export": "next build && next export",
    "test:e2e": "cross-env NEXT_DIST_DIR=.next-playwright PLAYWRIGHT=1 NEXT_PUBLIC_EZOIC_ENABLED=false npm run build && playwright test --workers=1",
    "test:unit": "node scripts/run-unit-tests.mjs",
    "check-contrast": "node scripts/check-contrast.js",
    "check-axe": "node scripts/check-axe.js",
    "security:scan": "node scripts/security-scan-no-pii.mjs",
    "ai:doctor": "tsx scripts/ai-doctor.ts",
    "ai:verify": "tsx scripts/ai-verify.ts",
    "ai:proof": "tsx scripts/ai-proof.ts",
    "export:pennycentral": "tsx scripts/export-pennycentral-json.ts",
    "validate:scrape": "tsx scripts/validate-scrape-json.ts",
    "convert:scrape": "tsx scripts/scrape-to-enrichment-csv.ts",
    "diff:enrichment": "tsx scripts/enrichment-diff.ts",
    "enrich:auto": "tsx scripts/auto-enrich.ts",
    "enrich:bulk": "tsx scripts/bulk-enrich.ts",
    "enrich:stealth": "tsx scripts/stealth-enrich.ts",
    "enrich:serpapi": "tsx scripts/serpapi-enrich.ts",
    "warm:staging": "node scripts/run-local-staging-warmer.mjs",
    "penny:count": "tsx scripts/print-penny-list-count.ts",
    "fixture:snapshot": "tsx scripts/snapshot-penny-list-fixture.ts",
    "prepare": "husky",
    "qa:fast": "npm run lint && npm run test:unit && npm run build",
    "qa:full": "npm run qa:fast && npm run test:e2e && npm run check-contrast && npm run check-axe"
  },
  "dependencies": {
    "@fontsource-variable/inter": "^5.2.8",
    "@hookform/resolvers": "^3.9.1",
    "@react-email/components": "^1.0.4",
    "@sentry/nextjs": "^10.32.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "@types/leaflet": "^1.9.21",
    "@vercel/analytics": "^1.6.1",
    "@vercel/postgres": "^0.10.0",
    "@vercel/speed-insights": "^1.3.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.26.2",
    "jsbarcode": "^3.12.3",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.562.0",
    "next": "16.1.3",
    "next-themes": "^0.4.6",
    "papaparse": "^5.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-email": "^5.2.5",
    "react-hook-form": "^7.53.2",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.6.0",
    "resend": "^6.7.0",
    "server-only": "^0.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@axe-core/cli": "^4.11.0",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/forms": "^0.5.11",
    "@types/node": "^22.19.5",
    "@types/papaparse": "^5.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vercel/og": "^0.8.6",
    "autoprefixer": "^10.4.23",
    "cross-env": "^7.0.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-node": "^0.3.9",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.0.0",
    "husky": "^9.1.7",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.2.7",
    "playwright-extra": "^4.3.6",
    "postcss": "^8.4.47",
    "prettier": "^3.8.0",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "satori": "^0.19.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.19",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.52.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,css,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": "22.x"
  },
  "overrides": {
    "import-in-the-middle": "2.0.1",
    "require-in-the-middle": "8.0.1"
  }
}
